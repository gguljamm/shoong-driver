<?xml version='1.0' encoding='utf-8'?>
<widget id="app.shoong.crew" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Shoong for crew</name>
    <description>
        슝 크루를 위한 앱
    </description>
    <author email="app@bringprice.com" href="http://cordova.io">
        (주)브링프라이스
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="AppendUserAgent" value="bpAos" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="AppendUserAgent" value="bpIos" />
    </platform>
</widget>
